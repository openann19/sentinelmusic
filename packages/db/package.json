{
  "name": "@music-hub/db",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "db:generate": "prisma generate --schema=./schema.prisma",
    "db:push": "prisma db push --schema=./schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=./schema.prisma",
    "db:studio": "prisma studio --schema=./schema.prisma",
    "db:seed": "tsx seed.ts",
    "postinstall": "prisma generate --schema=./schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
